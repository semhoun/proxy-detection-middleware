{
    "name": "akrabat/proxy-detection-middleware",
    "description": "PSR-7 Middleware that determines the scheme, host and port from the 'X-Forwarded-Proto', 'X-Forwarded-Host' and 'X-Forwarded-Port' headers and updates the Request's Uri object.",
    "keywords": [
        "psr7", "middleware", "ip"
    ],
    "homepage": "http://github.com/semhoun/proxy-detection-middleware",
    "type": "library",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Nathanael Semhoun",
            "email": "nathanael@semhoun.net",
            "homepage": "http://www.semhoun.net"
        },
        {
            "name": "Rob Allen",
            "email": "rob@akrabat.com",
            "homepage": "http://akrabat.com"
        }
    ],
    "require": {
        "php": "^7.2 || ^8.0",
        "psr/http-message": "^1.0",
        "psr/http-server-middleware": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8|^9",
		"middlewares/utils": "^3.0",
        "friendsofphp/php-cs-fixer": "^2.0",
        "squizlabs/php_codesniffer": "^3.0",
        "oscarotero/php-cs-fixer-config": "^1.0",
        "phpstan/phpstan": "^0.12",
        "laminas/laminas-diactoros": "^2.3"
    },
    "autoload": {
        "psr-4": {
            "RKA\\Middleware\\": "src"
        }
    }
}
